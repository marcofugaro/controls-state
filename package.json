{
  "name": "control-panel-2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "start": "budo --open --live --host localhost examples/demo3.js",
    "build": "npm run build:rollup",
    "build:example": "rm -rf docs && mkdir -p docs && browserify examples/demo3.js -t es2020 -t [ envify --NODE_ENV production ] | uglifyjs -cm |  indexhtmlify > docs/index.html",
    "build:rollup": "npm run bundle:rollup && npm run minify:rollup",
    "bundle:rollup": "mkdir -p dist && rollup -c",
    "minify:rollup": "uglifyjs -cm -o dist/gui.min.js dist/gui.js && uglifyjs -cm -o dist/state.min.js dist/state.js && uglifyjs -cm -o dist/gui.umd.min.js dist/gui.umd.js && uglifyjs -cm -o dist/state.umd.min.js dist/state.umd.js",
    "lint": "semistandard",
    "lint-fix": "semistandard --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "brfs": "^2.0.2",
    "browserify": "^16.2.2",
    "budo": "^11.6.1",
    "envify": "^4.1.0",
    "es2020": "^1.1.9",
    "h": "^0.1.0",
    "indexhtmlify": "^1.3.1",
    "json-beautify": "^1.0.1",
    "preact": "^8.3.0",
    "rollup": "^1.7.3",
    "rollup-plugin-browserify-transform": "^1.0.1",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "semistandard": "^12.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "uglify-js": "^3.4.7",
    "uglifyjs": "^2.4.11"
  },
  "dependencies": {
    "event-emitter": "^0.3.5",
    "global": "^4.3.2",
    "insert-css": "^2.0.0",
    "preact-classless-component": "^1.1.1",
    "raf": "^3.4.1",
    "rangetouch": "^1.0.5"
  }
}
